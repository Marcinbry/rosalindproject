import itertools

def permutacja(liczba):
    sum = 1
    lista = []
    for a in range(1, liczba + 1):
        lista.append(a)
        sum *= a
    print(sum)
    from itertools import permutations
    lista1 = "".join(map(str, lista))
    permList = permutations(lista1)
    for perm in list(permList):
            print(' '.join(perm))
print(permutacja(5))
