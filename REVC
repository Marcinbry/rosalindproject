#the string is unmodifiable, so you can add nucleotides to the list, then individual nucleotides are replaced with the right ones. You have to return it in the form of an inverted string so "".join(map(str, lista[::-1]))
def reverse(dna_seq):
    lista  = []
    for a in dna_seq:
        lista.append(a)
    for b in range(len(lista)):
        if lista[b] == "A":
            lista[b] = "T"
        elif lista[b] == "T":
            lista[b] = "A"
        elif lista[b] == "G":
            lista[b] = "C"
        elif lista[b] == "C":
            lista[b] = "G"
    sc = "".join(map(str, lista[::-1]))
    return sc
print(reverse(""))
