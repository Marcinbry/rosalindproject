#napis jest niemodyfikowalny, więc trzeba dodać zasady do listy, nastepnie poszczególne zasady są zamieniane na właściwe. Trzeba zwrócic to
#w postaci odwróconego  napisu dlatego "".join(map(str, lista[::-1]))
def reverse(odcinek):
    lista  = []
    for a in odcinek:
        lista.append(a)
    for b in range(len(lista)):
        if lista[b] == "A":
            lista[b] = "T"
        elif lista[b] == "T":
            lista[b] = "A"
        elif lista[b] == "G":
            lista[b] = "C"
        elif lista[b] == "C":
            lista[b] = "G"
    sc = "".join(map(str, lista[::-1]))
    return sc
print(reverse(""))
