def hammingdistance(odcinek1, odcinek2):
    lista1 = []
    lista2 = []
    sum = 0
    for a in odcinek1:
        lista1.append(a)
    for b in odcinek2:
        lista2.append(b)
    for c in range(len(lista1)):
        if lista1[c] != lista2[c]:
            sum += 1
    return sum







print(hammingdistance("", ""))

