#Lists were created from the given DNAs, then we compare them to each other, if they are not equal in a given place, the sum is increased by one
def hammingdistance(dna_seq1, dna_seq2):
    lista1 = []
    lista2 = []
    sum = 0
    for a in dna_seq1:
        lista1.append(a)
    for b in dna_seq2:
        lista2.append(b)
    for c in range(len(lista1)):
        if lista1[c] != lista2[c]:
            sum += 1
    return sum







print(hammingdistance("", ""))

